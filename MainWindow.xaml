<Window x:Class="PasswordGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordGenerator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
<StackPanel>
    <StackPanel>
        <TextBlock>Введите число</TextBlock>
        <TextBox Name="Tblenght"></TextBox>
    </StackPanel>
    <StackPanel>
        <TextBlock>Уровень сложности</TextBlock>
        <ComboBox Name="CbDiff" SelectedIndex="0">
            <ComboBoxItem>Лёгкий</ComboBoxItem>
            <ComboBoxItem>Средний</ComboBoxItem>
            <ComboBoxItem>Сложный</ComboBoxItem>
        </ComboBox>
    </StackPanel>
    
        <StackPanel>
            <TextBlock>Готовый пароль</TextBlock>
            <TextBox Name="TbBox"></TextBox>
            <Button Name="BtnGenerate" Click="BtnGenerate_OnClick">Сгенерировать</Button>
        </StackPanel>
     </StackPanel>
    </Grid>
</Window>
